{
  "extends": "airbnb-base",
  "parserOptions": {
    " sourceType": "module"
  },
  "rules": {
    "func-names": ["error", "never"],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": 1,
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "max-len": ["error", { "code": 140 }],
    "padded-blocks": ["error", { "classes": "always" }]
  }
}